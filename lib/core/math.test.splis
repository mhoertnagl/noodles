(do
  (use "core/prelude")
  (use "core/math")

  (test "sin (* (/ 0 4.0) *PI*)" 0 (sin (* (/ 0 4.0) *PI*)))
  ; (test "sin (* (/ 1 4.0) *PI*)" 0 (sin (* (/ 1 4.0) *PI*)))
  (test "sin (* (/ 2 4.0) *PI*)" 1 (sin (* (/ 2 4.0) *PI*)))
  ; (test "sin (* (/ 3 4.0) *PI*)" 0 (sin (* (/ 3 4.0) *PI*)))
  ; (test "sin (* (/ 4 4.0) *PI*)" 0 (sin (* (/ 4 4.0) *PI*)))
  ; (test "sin (* (/ 5 4.0) *PI*)" 0 (sin (* (/ 5 4.0) *PI*)))
  (test "sin (* (/ 6 4.0) *PI*)" -1 (sin (* (/ 6 4.0) *PI*)))
  ; (test "sin (* (/ 7 4.0) *PI*)" 0 (sin (* (/ 7 4.0) *PI*)))
  ; (test "sin (* (/ 8 4.0) *PI*)" 0 (sin (* (/ 8 4.0) *PI*)))

  (test "pow 2 0" 1 (pow 2 0))
  (test "pow 2 1" 2 (pow 2 1))
  (test "pow 2 2" 4 (pow 2 2))
  (test "pow 2 3" 8 (pow 2 3))
  (test "pow 2 31" 2147483648 (pow 2 31))
  (test "pow 2 32" 4294967296 (pow 2 32))
  (test "pow 2 33" 8589934592 (pow 2 33))
  (test "pow 3 0" 1 (pow 3 0))
  (test "pow 3 1" 3 (pow 3 1))
  (test "pow 3 2" 9 (pow 3 2))
  (test "pow 3 3" 27 (pow 3 3))
)
